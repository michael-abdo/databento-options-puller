# Databento API Configuration
DATABENTO_API_KEY=your_api_key_here

# Set to 'mock' for testing, 'live' for real data
DATA_MODE=mock

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Output settings
DEFAULT_OUTPUT_DIR=output
DEFAULT_DATE_FORMAT=%-m/%-d/%y

# Strategy settings
TARGET_DELTA=0.15
MONTHS_AHEAD=2
RISK_FREE_RATE=0.05